<li class="dropdown-item">
  <small class="text-muted"><%= a.created_at.strftime('%-d %B %Y %H:%M:%S') %></small><br/>
  <strong><%= activity.owner.full_name %></strong>
  <% if a.trackable %>
    added question <%= link_to a.trackable.title, question_path(a.trackable) %>
  <% else %>
    added a question that has been deleted
  <% end %>
</li>